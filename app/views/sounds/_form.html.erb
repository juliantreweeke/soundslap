
<%= form_for @sound, html: {multipart: true} do |f| %>

  <%= f.label :sound %>
  <%= f.cl_image_upload :sound %>
  <br><br>

  <%= f.label :title %>
  <%= f.text_field :title %>
  <br><br>

  <%= f.label :info %>
  <%= f.text_field :description %>
  <br><br>

  <%= f.label :image%>
  <%= f.text_field :image %>
  <br><br>

  <br>

  <br>

  <%= f.submit %>

<% end %>
